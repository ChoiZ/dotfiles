#!/bin/sh

tmux -2 new -d -c "$HOME" -n "root" -s "dev"
tmux send-keys -t dev:root su Enter
tmux neww -c "$HOME/docker/www/manomano-sf" -n "dev"
tmux neww -c "$HOME/docker" -n "docker"
tmux send-keys -t dev:docker "d-c up" Enter
tmux neww -c "$HOME" -n "preprod"
tmux send-keys -t dev:preprod sshmm Enter
tmux attach